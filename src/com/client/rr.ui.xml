<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	  xmlns:m='urn:import:gwt.material.design.client.ui'
             xmlns:ma="urn:import:gwt.material.design.addins.client">

	<ui:style gss="true">
        body {
            background: #e9e9e9;
        }
    </ui:style>
    <g:HTMLPanel>
    <m:MaterialPanel>
        <m:MaterialHeader ui:field="header">
            <m:MaterialNavBar activates="sidenavColaps" backgroundColor="BLUE" shadow="1">
                <m:MaterialNavBrand paddingLeft="60" text="BrainPop" />
            </m:MaterialNavBar>
        </m:MaterialHeader>

        <m:MaterialSideNavPush m:id="sidenavColaps" ui:field="sideNav" width="300">
            <ma:sideprofile.MaterialSideProfile url="http://static1.squarespace.com/static/51609147e4b0715db61d32b6/521b97cee4b05f031fd12dde/5519e33de4b06a1002802805/1431718693701/?format=1500w">
                <m:MaterialImage url="http://b.vimeocdn.com/ps/339/488/3394886_300.jpg" />
                <m:MaterialLabel text="BrainPop" textColor="WHITE"/>
                <m:MaterialLink text="Brainpop@gmail.com" activates="dropProfile" iconType="ARROW_DROP_DOWN" iconPosition="RIGHT" textColor="WHITE"/>
            </ma:sideprofile.MaterialSideProfile>
            <m:MaterialCollapsible>
                <!-- PARENT 1 -->
                <m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleHeader waves="DEFAULT">
                        <m:MaterialLink  text="Quiz" iconType="POLYMER"/>
                    </m:MaterialCollapsibleHeader>
                    <m:MaterialCollapsibleBody>
                        <m:html.UnorderedList>
                            <m:MaterialLink iconType="POLYMER" text="MCQ"  ui:field="t1" waves="DEFAULT" />
                            <m:MaterialLink iconType="POLYMER" text="SEQUENCE" ui:field="t2" waves="DEFAULT" />
                        </m:html.UnorderedList>
                    </m:MaterialCollapsibleBody>
                </m:MaterialCollapsibleItem>
                <!-- PARENT 2 -->
                <m:MaterialCollapsibleItem>
                    <m:MaterialCollapsibleHeader waves="DEFAULT">
                        <m:MaterialLink text="Live Lecturs" iconType="POLYMER"/>
                    </m:MaterialCollapsibleHeader>
                    <m:MaterialCollapsibleBody>
                        <m:html.UnorderedList>
                            <m:MaterialLink iconType="POLYMER" text="Java" waves="DEFAULT" />
                            <m:MaterialLink iconType="POLYMER" text="Python" waves="DEFAULT" />
                        </m:html.UnorderedList>
                    </m:MaterialCollapsibleBody>
                </m:MaterialCollapsibleItem>
                <!-- PARENT 3 -->
               
            </m:MaterialCollapsible>
        </m:MaterialSideNavPush>
	<m:MaterialPanel ui:field="con" m:id="tt1">
        <m:MaterialContainer>
            <m:MaterialRow textAlign="CENTER" paddingTop="40">
                <m:MaterialImage url="http://i.imgur.com/7JpG0F4.gif" />
                <m:MaterialFAB>
                    <m:MaterialButton type="FLOATING" href="https://github.com/GwtMaterialDesign/gwt-material-patterns/blob/release_2.0/src/main/java/com/github/gwtmaterialdesign/client/application/sidenavdrawer/DrawerSideNavView.ui.xml" target="_blank" backgroundColor="PINK" size="LARGE" iconType="CODE" />
                </m:MaterialFAB>
            </m:MaterialRow>
        </m:MaterialContainer></m:MaterialPanel>
    </m:MaterialPanel>
	
	</g:HTMLPanel>
</ui:UiBinder> 